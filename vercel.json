{
  "version": 2,
  "builds": [
    {
      "src": "servers/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "client/package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/servers/(.*)", 
      "dest": "/servers/index.js"
    },
    {
      "src": "/(.*)",    
      "dest": "/client/$1"
    }
  ]
}
